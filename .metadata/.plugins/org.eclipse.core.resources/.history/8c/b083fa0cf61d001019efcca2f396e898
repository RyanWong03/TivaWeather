#ifndef GPIO_H_
#define GPIO_H_

#define RCGCGPIO    0x400FE608

#define GPIOPORTE   0x4005C000

#define GPIOAFSEL   0x420
#define GPIODEN     0x51C
#define GPIOAMSEL   0x528

#define PORTA       0x1
#define PORTK       0x200
#define PORTE       0x10


void gpio_port_init(int port);

#endif /* GPIO_H_ */
