#include "main.h"

//LCD SPI clock =   PQ0
//LCD SPI CS    =   PP3
//LCD SPI MOSI  =   PQ2
//LCD reset     =
//LCD RS        =   PK7
void LCD_init()
{
    //Initialize SSI Module 3. LCD is connected to this.
    (*((volatile uint32_t *)(0x400FE61C))) |= 8;

    gpio_port_init(PORTK | PORTP | PORTQ);

    //Set PQ0 & PQ2 as digital.
    (*((volatile uint32_t *)(GPIOPORTQ + GPIODEN))) |= ;
}
